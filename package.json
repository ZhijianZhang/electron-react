{
  "name": "electron-react",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": ".",
  "dependencies": {
    "antd": "^3.5.1",
    "electron": "^2.0.0",
    "mobx": "^4.2.1",
    "mobx-react": "^5.1.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-start": "electron . --env dev",
    "electron-build": "electron-packager . HelloWorld --win --out ./output --version 2.0.0 --overwrite --icon=./public/favicon.ico --ignore=node_modules",
    "all-build": "npm run build && electron-packager . HelloWorld --all --out ./output --arch=x64 --version 2.0.0 --overwrite --icon=./public/favicon.ico --ignore=node_modules"
  },
  "devDependencies": {
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "electron-packager": "^12.0.2",
    "react-app-rewired": "^1.5.2"
  },
  "babel": {
    "plugins": [
      "transform-decorators-legacy"
    ]
  }
}
